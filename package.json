{
  "name": "million_technical_test",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install-proyect": "yarn && cd ios && pod install && cd ..",
    "start": "react-native start --reset-cache",
    "android:development": "npx react-native run-android --mode=developmentdebug --appIdSuffix=development --active-arch-only",
    "android:production": "npx react-native run-android --mode=productiondebug --active-arch-only",
    "android:development-release": "npx react-native run-android --mode=developmentrelease --appIdSuffix=development --active-arch-only",
    "android:production-release": "npx react-native run-android --mode=productionrelease --active-arch-only",
    "ios:development": "react-native run-ios --simulator=\"iPhone 16 Pro\" --scheme \"million_technical_testDev\" ",
    "ios:production": "react-native run-ios --simulator=\"iPhone 16 Pro\" --scheme \"million_technical_test\" ",
    "lint": "eslint .",
    "test": "jest",
    "xcode": "xed ios",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "generate-pr-template": "node ./scripts/generate-pr-template.js",
    "verify-branch-name": "node ./scripts/verify-branch-name.js"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.12",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-config": "^1.5.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
